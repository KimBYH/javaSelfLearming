{{>layouts/header}}

<table class="table">
    <thead>
    <tr>
        <th scope="col">Id</th>
        <th scope="col">Email</th>
        <th scope="col">Password</th>
    </tr>
    </thead>
    <tbody>
    {{#membersList}}
        <tr>
            <td>{{id}}</td>
            <td><a href="/members/{{id}}">{{email}}</a></td>
            <td>{{password}}</td>
        </tr>
    {{/membersList}}
    </tbody>
</table>
<button type="button" class="btn btn-primary" onclick="location.href='/members/new'">New Member</button>


{{>layouts/footer}}